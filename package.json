{
	"name": "snuboard-crawler",
	"version": "0.0.2",
	"description": "This is the crawler for SNUBoard project.",
	"dependencies": {
		"@octokit/auth-app": "^3.0.0",
		"@octokit/core": "^3.2.5",
		"apify": "^1.0.1",
		"bcrypt": "^5.0.0",
		"better-sqlite3-with-prebuilds": "^7.1.1",
		"cheerio": "^1.0.0-rc.3",
		"class-transformer": "^0.3.2",
		"dotenv": "^8.2.0",
		"mysql2": "^2.2.5",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.31"
	},
	"devDependencies": {
		"@apify/eslint-config": "^0.1.3",
		"@types/bcrypt": "^3.0.0",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.25",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"eslint": "^7.19.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"husky": "^5.1.3",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1",
		"ts-jest": "^26.5.1",
		"typescript": "^4.1.5"
	},
	"scripts": {
		"prestart": "npm run build",
		"start": "node dist/src/main.js",
		"lint": "eslint \"{src,test}/**/*.{js,ts,tsx}\" --fix",
		"lint-staged": "lint-staged",
		"build": "tsc",
		"rebuild": "rm -rf dist/* && npm run build",
		"clean": "rm -rf apify_storage",
		"test": "jest",
		"coverage": "jest --coverage",
		"update": "git submodule update --remote",
		"precrawl-one": "tsc",
		"crawl-one": "node dist/src/crawl-one.js"
	},
	"lint-staged": {
		"src/**/*.{js,json,ts}": [
			"prettier --write"
		]
	},
